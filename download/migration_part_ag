// Helper functions
export const getCardsByCategory = (category: KnowledgeCategory): KnowledgeCard[] => 
  knowledgeCards.filter(card => card.category === category);

export const getCardsByDifficulty = (difficulty: DifficultyLevel): KnowledgeCard[] => 
  knowledgeCards.filter(card => card.difficulty === difficulty);

export const getCardById = (id: string): KnowledgeCard | undefined => 
  knowledgeCards.find(card => card.id === id);

export const searchCards = (query: string): KnowledgeCard[] => {
  const lowercaseQuery = query.toLowerCase();
  return knowledgeCards.filter(card => 
    card.title.toLowerCase().includes(lowercaseQuery) ||
    card.titleMm.includes(query) ||
    card.summary.toLowerCase().includes(lowercaseQuery) ||
    card.tags.some(tag => tag.toLowerCase().includes(lowercaseQuery))
  );
};

export const getRandomCards = (count: number): KnowledgeCard[] => {
  const shuffled = [...knowledgeCards].sort(() => Math.random() - 0.5);
  return shuffled.slice(0, count);
};

// Statistics
export const getCardStats = () => ({
  total: knowledgeCards.length,
  byCategory: {
    tech: getCardsByCategory('tech').length,
    business: getCardsByCategory('business').length,
    economy: getCardsByCategory('economy').length,
    skills: getCardsByCategory('skills').length,
    career: getCardsByCategory('career').length,
    life: getCardsByCategory('life').length,
  },
  byDifficulty: {
    beginner: getCardsByDifficulty('beginner').length,
    intermediate: getCardsByDifficulty('intermediate').length,
    advanced: getCardsByDifficulty('advanced').length,
  },
  totalPoints: knowledgeCards.reduce((sum, card) => sum + card.points, 0),
});
=== END FILE ===

=== FILE: src/data/zodiac.ts ===
// Zodiac Career Insights Data
// Connecting astrology to career development for Myanmar youth

export interface ZodiacSign {
  id: string;
  name: string;
  symbol: string;
  element: 'Fire' | 'Earth' | 'Air' | 'Water';
  quality: 'Cardinal' | 'Fixed' | 'Mutable';
  dates: string;
  rulingPlanet: string;
  traits: {
    positive: string[];
    negative: string[];
  };
  career: {
    strengths: string[];
    developmentAreas: string[];
    idealCareers: string[];
    workStyle: string;
    leadershipStyle: string;
  };
  softSkills: {
    natural: string[];
    toDevelop: string[];
  };
  recommendedCourses: string[];
  monthlyHoroscope: {
    career: string;
    love: string;
    health: string;
    lucky: {
      number: string;
      color: string;
      day: string;
    };
  };
  famousPeople: string[];
  compatibility: {
    best: string[];
    good: string[];
    challenging: string[];
  };
}

