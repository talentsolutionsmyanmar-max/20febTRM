# ===========================================
# SUPABASE DATABASE CONFIGURATION
# ===========================================
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/database

# Session Pooler (for migrations) - port 5432
DATABASE_URL="postgresql://postgres.PROJECT_REF:[YOUR-PASSWORD]@aws-1-ap-southeast-1.pooler.supabase.com:5432/postgres"

# Direct connection (same as above for Prisma)
DIRECT_URL="postgresql://postgres.PROJECT_REF:[YOUR-PASSWORD]@aws-1-ap-southeast-1.pooler.supabase.com:5432/postgres"

# ===========================================
# SUPABASE CLIENT CONFIGURATION
# ===========================================
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api

NEXT_PUBLIC_SUPABASE_URL="https://PROJECT_REF.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"
